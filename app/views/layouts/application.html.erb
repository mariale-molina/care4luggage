<!DOCTYPE html>
<html>
<head>
  <title>Care4luggage</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  
  <%= csrf_meta_tags %>
</head>
<body>
 <header>
    <div class="header col-xs-12">
      <%= render partial: "shared/nav_lenguages"%>
      <%= render partial: "shared/nav"%>   
    </div>
  </header>

  <div class="section-1 col-xs-12">

    <div class="col-xs-12 owl-carousel owl-theme">

      <div class="item">
        <img src="/img/b1.jpg"></img>
      </div>

      <div class="item">
        <img src="/img/b2.jpg"></img>
      </div>

      <div class="item">
        <img src="/img/b3.jpg"></img>
      </div>

    </div>
  </div>

    <%= yield %>

  <div class="footer"> 
    <div class="sas"> 
      <p>header 1</p>
    </div>
    <%= render partial: "shared/footer_payment.slim"%>
  </div>


</body>
</html>
